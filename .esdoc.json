{
    "source": ".",
    "includes": ["\\.js$"],
    "excludes": [
        "node_modules/*",
        "jspm_packages/*",
        "docs/*",
        "index.js",
        "gulpfile.js",
        "core/js/config.js",
        "varsity18/"
    ],
    "destination": "./docs",
    "plugins": [
        { "name": "esdoc-standard-plugin",
        "option": {
            "unexportedIdentifier": { "enable": false },
            "undocumentIdentifier": { "enable": true }
        }}
    ]
}
